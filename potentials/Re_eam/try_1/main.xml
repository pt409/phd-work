<?xml version='1.0' encoding='iso-8859-1'?>
<job>

  <name>Fitting an EAM potential for rhenium</name>
  
  <verbosity>medium</verbosity>
  
  <fitting>
    <BFGS conv-threshold='1e-4' max-iter='85' />
  </fitting>
  
  <atom-types>
    <species>Re</species>
  </atom-types>
  
  <potentials>
    <xi:include href='potential.xml'
		xmlns:xi='http://www.w3.org/2003/XInclude' />
  </potentials>
  
  <structures>
    <xi:include href='structures.xml'
		xmlns:xi='http://www.w3.org/2003/XInclude' />
  </structures>

</job>
