[DEFAULT]
incl_ht = true
learn_log_Ki = true
comp_kernel_type = special
ht_kernel_type = rbf
output_models = rigorous/final_models.pkl
database = ../datasets/processing/processed_alloy_database_v4.csv
test_pc = 13.0
seed = 2106
error_weight = 1.0
standardise_ht = true
standardise_comp = false
prelim_search = true

