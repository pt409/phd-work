output_models = kernel_tests/all_rbf_model.pkl

[physical_ht_kernel]
ht_kernel_type = unordered
output_models = kernel_tests/physical_ht_kernel_model.pkl

[poly_ht_kernel]
ht_kernel_type = poly
output_models = kernel_tests/poly_ht_model.pkl
