comp_kernel_type = rbf
output_models = kernel_tests/gauss_rbf_model.pkl

[special_rbf_test]
incl_ht = false
comp_kernel_type = special
output_models = kernel_tests/special_rbf_model.pkl

[rbf_ht_kernel]
ht_kernel_type = rbf
output_models = kernel_tests/rbf_ht_model.pkl

[all_rbf_kernel]
ht_kernel_type = rbf
